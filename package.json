{
  "name": "jenkins-workbench",
  "displayName": "Jenkins Workbench",
  "publisher": "airizom",
  "description": "Browse Jenkins instances, trigger builds, view logs, and manage CI/CD pipelines from VS Code.",
  "icon": "resources/icon.png",
  "version": "1.29.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Airizom/jenkins-workbench.git"
  },
  "bugs": {
    "url": "https://github.com/Airizom/jenkins-workbench/issues"
  },
  "homepage": "https://github.com/Airizom/jenkins-workbench#readme",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other",
    "Visualization"
  ],
  "keywords": [
    "jenkins",
    "ci",
    "cd",
    "cicd",
    "build",
    "pipeline",
    "devops",
    "automation",
    "continuous integration"
  ],
  "galleryBanner": {
    "color": "#1a1a2e",
    "theme": "dark"
  },
  "activationEvents": [
    "onStartupFinished",
    "onUri"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "uriHandler": {
      "scheme": "airizom.jenkins-workbench"
    },
    "commands": [
      {
        "command": "jenkinsWorkbench.addEnvironment",
        "title": "Jenkins: Add Environment",
        "icon": "$(add)"
      },
      {
        "command": "jenkinsWorkbench.removeEnvironment",
        "title": "Jenkins: Remove Environment",
        "icon": "$(trash)"
      },
      {
        "command": "jenkinsWorkbench.triggerBuild",
        "title": "Jenkins: Trigger Build",
        "icon": "$(play)"
      },
      {
        "command": "jenkinsWorkbench.abortBuild",
        "title": "Jenkins: Abort/Stop Build",
        "icon": "$(debug-stop)"
      },
      {
        "command": "jenkinsWorkbench.approveInput",
        "title": "Jenkins: Approve Input",
        "icon": "$(check)"
      },
      {
        "command": "jenkinsWorkbench.rejectInput",
        "title": "Jenkins: Reject Input",
        "icon": "$(close)"
      },
      {
        "command": "jenkinsWorkbench.cancelQueueItem",
        "title": "Jenkins: Cancel Queue Item",
        "icon": "$(close)"
      },
      {
        "command": "jenkinsWorkbench.replayBuild",
        "title": "Jenkins: Replay Build"
      },
      {
        "command": "jenkinsWorkbench.rebuildBuild",
        "title": "Jenkins: Rebuild"
      },
      {
        "command": "jenkinsWorkbench.openInJenkins",
        "title": "Jenkins: Open in Jenkins",
        "icon": "$(link-external)"
      },
      {
        "command": "jenkinsWorkbench.goToJob",
        "title": "Jenkins: Go to Job...",
        "icon": "$(search)"
      },
      {
        "command": "jenkinsWorkbench.filterJobsAll",
        "title": "Jenkins: Show All Jobs",
        "icon": "$(list-flat)"
      },
      {
        "command": "jenkinsWorkbench.filterJobsFailing",
        "title": "Jenkins: Show Failing Jobs",
        "icon": "$(error)"
      },
      {
        "command": "jenkinsWorkbench.filterJobsRunning",
        "title": "Jenkins: Show Running Jobs",
        "icon": "$(play)"
      },
      {
        "command": "jenkinsWorkbench.filterJobs",
        "title": "Jenkins: Filter Jobs",
        "icon": "$(filter)"
      },
      {
        "command": "jenkinsWorkbench.filterJobsActive",
        "title": "Jenkins: Filter Jobs",
        "icon": "$(filter-filled)"
      },
      {
        "command": "jenkinsWorkbench.showBuildDetails",
        "title": "Jenkins: View Build Details"
      },
      {
        "command": "jenkinsWorkbench.showNodeDetails",
        "title": "Jenkins: View Node Details"
      },
      {
        "command": "jenkinsWorkbench.takeNodeOffline",
        "title": "Jenkins: Take Node Offline...",
        "icon": "$(debug-disconnect)"
      },
      {
        "command": "jenkinsWorkbench.bringNodeOnline",
        "title": "Jenkins: Bring Node Online",
        "icon": "$(debug-connect)"
      },
      {
        "command": "jenkinsWorkbench.launchNodeAgent",
        "title": "Jenkins: Launch Node Agent",
        "icon": "$(debug-start)"
      },
      {
        "command": "jenkinsWorkbench.previewBuildLog",
        "title": "Jenkins: Preview Build Logs",
        "icon": "$(terminal)"
      },
      {
        "command": "jenkinsWorkbench.openLastFailedBuild",
        "title": "Jenkins: Open Last Failed Build"
      },
      {
        "command": "jenkinsWorkbench.viewJobConfig",
        "title": "Jenkins: View Job Config",
        "icon": "$(file-code)"
      },
      {
        "command": "jenkinsWorkbench.updateJobConfig",
        "title": "Jenkins: Update Job Config",
        "icon": "$(edit)"
      },
      {
        "command": "jenkinsWorkbench.submitJobConfig",
        "title": "Jenkins: Submit Job Config",
        "icon": "$(check)"
      },
      {
        "command": "jenkinsWorkbench.previewArtifact",
        "title": "Jenkins: Preview Artifact",
        "icon": "$(eye)"
      },
      {
        "command": "jenkinsWorkbench.downloadArtifact",
        "title": "Jenkins: Download Artifact",
        "icon": "$(cloud-download)"
      },
      {
        "command": "jenkinsWorkbench.filterBranches",
        "title": "Jenkins: Filter Branches"
      },
      {
        "command": "jenkinsWorkbench.clearBranchFilter",
        "title": "Jenkins: Clear Branch Filter"
      },
      {
        "command": "jenkinsWorkbench.watchJob",
        "title": "Jenkins: Watch Job"
      },
      {
        "command": "jenkinsWorkbench.unwatchJob",
        "title": "Jenkins: Unwatch Job"
      },
      {
        "command": "jenkinsWorkbench.pinJob",
        "title": "Jenkins: Pin Job",
        "icon": "$(pin)"
      },
      {
        "command": "jenkinsWorkbench.unpinJob",
        "title": "Jenkins: Unpin Job",
        "icon": "$(pinned)"
      },
      {
        "command": "jenkinsWorkbench.refresh",
        "title": "Jenkins: Refresh",
        "icon": "$(refresh)"
      },
      {
        "command": "jenkinsWorkbench.enableJob",
        "title": "Jenkins: Enable Job",
        "icon": "$(play-circle)"
      },
      {
        "command": "jenkinsWorkbench.disableJob",
        "title": "Jenkins: Disable Job",
        "icon": "$(debug-pause)"
      },
      {
        "command": "jenkinsWorkbench.renameJob",
        "title": "Jenkins: Rename Job",
        "icon": "$(edit)"
      },
      {
        "command": "jenkinsWorkbench.deleteJob",
        "title": "Jenkins: Delete Job",
        "icon": "$(trash)"
      },
      {
        "command": "jenkinsWorkbench.copyJob",
        "title": "Jenkins: Copy Job",
        "icon": "$(copy)"
      },
      {
        "command": "jenkinsWorkbench.jenkinsfile.validateActive",
        "title": "Jenkins: Validate Jenkinsfile",
        "icon": "$(check)"
      },
      {
        "command": "jenkinsWorkbench.jenkinsfile.selectValidationEnvironment",
        "title": "Jenkins: Select Jenkinsfile Validation Environment"
      },
      {
        "command": "jenkinsWorkbench.jenkinsfile.clearDiagnostics",
        "title": "Jenkins: Clear Jenkinsfile Validation Diagnostics"
      },
      {
        "command": "jenkinsWorkbench.jenkinsfile.showValidationOutput",
        "title": "Jenkins: Show Jenkinsfile Validation Output"
      }
    ],
    "taskDefinitions": [
      {
        "type": "jenkinsWorkbench",
        "required": [
          "environmentUrl",
          "jobUrl"
        ],
        "properties": {
          "environmentUrl": {
            "type": "string",
            "description": "Base Jenkins URL for the environment."
          },
          "jobUrl": {
            "type": "string",
            "description": "Job URL or path relative to environmentUrl (for example: job/folder/job/name/)."
          },
          "environmentId": {
            "type": "string",
            "description": "Optional environment id to disambiguate when multiple environments share a URL."
          },
          "parameters": {
            "type": "object",
            "description": "Optional build parameters passed to Jenkins.",
            "additionalProperties": {
              "type": [
                "string",
                "number",
                "boolean",
                "array"
              ],
              "items": {
                "type": [
                  "string",
                  "number",
                  "boolean"
                ]
              }
            }
          }
        }
      }
    ],
    "keybindings": [
      {
        "command": "jenkinsWorkbench.jenkinsfile.validateActive",
        "key": "ctrl+shift+j",
        "mac": "cmd+shift+j",
        "when": "editorTextFocus && jenkinsWorkbench.jenkinsfileActive"
      }
    ],
    "configuration": {
      "title": "Jenkins Workbench",
      "properties": {
        "jenkinsWorkbench.cacheTtlSeconds": {
          "type": "number",
          "default": 300,
          "minimum": 0,
          "description": "Cache TTL in seconds for Jenkins data (jobs, builds, nodes, parameters). Use 0 to disable caching."
        },
        "jenkinsWorkbench.pollIntervalSeconds": {
          "type": "number",
          "default": 60,
          "minimum": 5,
          "description": "Polling interval in seconds for watched jobs."
        },
        "jenkinsWorkbench.queuePollIntervalSeconds": {
          "type": "number",
          "default": 10,
          "minimum": 2,
          "description": "Polling interval in seconds for the build queue while the Build Queue folder is expanded."
        },
        "jenkinsWorkbench.watchErrorThreshold": {
          "type": "number",
          "default": 3,
          "minimum": 1,
          "description": "Consecutive polling errors before warning about a watched job. Watches are removed only when Jenkins reports the job missing."
        },
        "jenkinsWorkbench.buildDetailsRefreshIntervalSeconds": {
          "type": "number",
          "default": 5,
          "minimum": 2,
          "description": "Polling interval in seconds for refreshing build details and streaming logs."
        },
        "jenkinsWorkbench.buildDetails.testReport.includeCaseLogs": {
          "type": "boolean",
          "default": false,
          "description": "Include per-test stack traces and stdout/stderr when fetching test reports. Enabling this can increase payload size for large test suites."
        },
        "jenkinsWorkbench.jobSearchConcurrency": {
          "type": "number",
          "default": 4,
          "minimum": 1,
          "description": "Maximum number of concurrent folder requests per environment while indexing jobs for Go to Job."
        },
        "jenkinsWorkbench.jobSearchBackoffBaseMs": {
          "type": "number",
          "default": 200,
          "minimum": 0,
          "description": "Base delay in milliseconds used when Jenkins responds with transient errors during job search."
        },
        "jenkinsWorkbench.jobSearchBackoffMaxMs": {
          "type": "number",
          "default": 2000,
          "minimum": 0,
          "description": "Maximum delay in milliseconds for adaptive backoff during job search."
        },
        "jenkinsWorkbench.jobSearchMaxRetries": {
          "type": "number",
          "default": 2,
          "minimum": 0,
          "description": "Maximum number of retries for transient Jenkins errors (429/5xx) during job search."
        },
        "jenkinsWorkbench.requestTimeoutSeconds": {
          "type": "number",
          "default": 30,
          "minimum": 5,
          "maximum": 300,
          "description": "Request timeout in seconds for Jenkins API calls."
        },
        "jenkinsWorkbench.artifactDownloadRoot": {
          "type": "string",
          "default": "jenkins-artifacts",
          "description": "Workspace-relative folder for downloaded artifacts."
        },
        "jenkinsWorkbench.artifactMaxDownloadMb": {
          "type": "number",
          "default": 100,
          "minimum": 0,
          "description": "Maximum artifact download size in megabytes. Set to 0 to disable the limit."
        },
        "jenkinsWorkbench.artifactPreviewCacheMaxEntries": {
          "type": "number",
          "default": 50,
          "minimum": 1,
          "description": "Maximum number of in-memory artifact previews to keep before eviction."
        },
        "jenkinsWorkbench.artifactPreviewCacheMaxMb": {
          "type": "number",
          "default": 200,
          "minimum": 1,
          "description": "Maximum total size in megabytes for in-memory artifact previews."
        },
        "jenkinsWorkbench.artifactPreviewCacheTtlSeconds": {
          "type": "number",
          "default": 900,
          "minimum": 60,
          "description": "Time-to-live in seconds for unused in-memory artifact previews."
        },
        "jenkinsWorkbench.maxCacheEntries": {
          "type": "number",
          "default": 1000,
          "minimum": 100,
          "description": "Maximum number of entries to keep in the data cache. Older entries are evicted when the limit is reached."
        },
        "jenkinsWorkbench.buildTooltips.includeDetails": {
          "type": "boolean",
          "default": false,
          "description": "Fetch additional build details (change sets, causes, parameters, estimated duration) for tree tooltips. Enabling this may increase build list response sizes."
        },
        "jenkinsWorkbench.buildTooltips.parameters.enabled": {
          "type": "boolean",
          "default": false,
          "description": "Show build parameters in build tooltips. Parameter values may be sensitive."
        },
        "jenkinsWorkbench.buildTooltips.parameters.allowList": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [],
          "description": "Only show parameters whose names contain any of these case-insensitive substrings."
        },
        "jenkinsWorkbench.buildTooltips.parameters.denyList": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [],
          "description": "Hide parameters whose names contain any of these case-insensitive substrings."
        },
        "jenkinsWorkbench.buildTooltips.parameters.maskPatterns": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [
            "password",
            "token",
            "secret",
            "apikey",
            "api_key",
            "credential",
            "passphrase"
          ],
          "description": "Mask parameter values when their names contain any of these case-insensitive substrings."
        },
        "jenkinsWorkbench.buildTooltips.parameters.maskValue": {
          "type": "string",
          "default": "[redacted]",
          "description": "Replacement text for masked parameter values."
        },
        "jenkinsWorkbench.jenkinsfileValidation.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable Jenkinsfile validation using the Jenkins declarative linter."
        },
        "jenkinsWorkbench.jenkinsfileValidation.runOnSave": {
          "type": "boolean",
          "default": true,
          "description": "Validate Jenkinsfiles automatically on save."
        },
        "jenkinsWorkbench.jenkinsfileValidation.changeDebounceMs": {
          "type": "number",
          "default": 500,
          "minimum": 0,
          "description": "Debounce delay in milliseconds before validating Jenkinsfiles on change (0 = immediate)."
        },
        "jenkinsWorkbench.jenkinsfileValidation.filePatterns": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [
            "**/Jenkinsfile",
            "**/*.jenkinsfile",
            "**/Jenkinsfile.*"
          ],
          "description": "Glob patterns used to detect Jenkinsfile documents for validation."
        }
      }
    },
    "menus": {
      "view/title": [
        {
          "command": "jenkinsWorkbench.addEnvironment",
          "when": "view == jenkinsWorkbench.tree",
          "group": "navigation@0"
        },
        {
          "command": "jenkinsWorkbench.refresh",
          "when": "view == jenkinsWorkbench.tree",
          "group": "navigation@1"
        },
        {
          "command": "jenkinsWorkbench.goToJob",
          "when": "view == jenkinsWorkbench.tree",
          "group": "navigation@2"
        },
        {
          "command": "jenkinsWorkbench.filterJobs",
          "when": "view == jenkinsWorkbench.tree && jenkinsWorkbench.filterMode == all",
          "group": "navigation@3"
        },
        {
          "command": "jenkinsWorkbench.filterJobsActive",
          "when": "view == jenkinsWorkbench.tree && jenkinsWorkbench.filterMode != all",
          "group": "navigation@3"
        }
      ],
      "view/item/context": [
        {
          "command": "jenkinsWorkbench.refresh",
          "when": "view == jenkinsWorkbench.tree && viewItem == environment",
          "group": "inline"
        },
        {
          "command": "jenkinsWorkbench.removeEnvironment",
          "when": "view == jenkinsWorkbench.tree && viewItem == environment",
          "group": "navigation"
        },
        {
          "command": "jenkinsWorkbench.triggerBuild",
          "when": "view == jenkinsWorkbench.tree && viewItem =~ /(^|\\s)(jobItem|pipelineItem)(\\s|$)/",
          "group": "inline@0"
        },
        {
          "command": "jenkinsWorkbench.openInJenkins",
          "when": "view == jenkinsWorkbench.tree && viewItem =~ /(^|\\s)(jobItem|pipelineItem)(\\s|$)/",
          "group": "inline@1"
        },
        {
          "command": "jenkinsWorkbench.openInJenkins",
          "when": "view == jenkinsWorkbench.tree && viewItem =~ /(^|\\s)nodeOpenable(\\s|$)/",
          "group": "inline@0"
        },
        {
          "command": "jenkinsWorkbench.showNodeDetails",
          "when": "view == jenkinsWorkbench.tree && viewItem =~ /(^|\\s)node(\\s|$)/",
          "group": "1_actions@0"
        },
        {
          "command": "jenkinsWorkbench.takeNodeOffline",
          "when": "view == jenkinsWorkbench.tree && viewItem =~ /(^|\\s)nodeOnline(\\s|$)/ && viewItem =~ /(^|\\s)nodeOpenable(\\s|$)/",
          "group": "1_actions@1"
        },
        {
          "command": "jenkinsWorkbench.bringNodeOnline",
          "when": "view == jenkinsWorkbench.tree && viewItem =~ /(^|\\s)nodeTemporarilyOffline(\\s|$)/ && viewItem =~ /(^|\\s)nodeOpenable(\\s|$)/",
          "group": "1_actions@1"
        },
        {
          "command": "jenkinsWorkbench.launchNodeAgent",
          "when": "view == jenkinsWorkbench.tree && viewItem =~ /(^|\\s)nodeLaunchable(\\s|$)/ && viewItem =~ /(^|\\s)nodeOpenable(\\s|$)/",
          "group": "1_actions@2"
        },
        {
          "command": "jenkinsWorkbench.pinJob",
          "when": "view == jenkinsWorkbench.tree && viewItem =~ /(^|\\s)(jobItem|pipelineItem)(\\s|$)/ && !(viewItem =~ /(^|\\s)pinned(\\s|$)/)",
          "group": "inline@2"
        },
        {
          "command": "jenkinsWorkbench.unpinJob",
          "when": "view == jenkinsWorkbench.tree && viewItem =~ /(^|\\s)(jobItem|pipelineItem)(\\s|$)/ && viewItem =~ /(^|\\s)pinned(\\s|$)/",
          "group": "inline@2"
        },
        {
          "command": "jenkinsWorkbench.openLastFailedBuild",
          "when": "view == jenkinsWorkbench.tree && viewItem =~ /(^|\\s)(jobItem|pipelineItem)(\\s|$)/",
          "group": "1_actions@0"
        },
        {
          "command": "jenkinsWorkbench.viewJobConfig",
          "when": "view == jenkinsWorkbench.tree && viewItem =~ /(^|\\s)(jobItem|pipelineItem)(\\s|$)/",
          "group": "1_actions@1"
        },
        {
          "command": "jenkinsWorkbench.updateJobConfig",
          "when": "view == jenkinsWorkbench.tree && viewItem =~ /(^|\\s)(jobItem|pipelineItem)(\\s|$)/",
          "group": "1_actions@2"
        },
        {
          "command": "jenkinsWorkbench.enableJob",
          "when": "view == jenkinsWorkbench.tree && viewItem =~ /(^|\\s)(jobItem|pipelineItem)(\\s|$)/ && viewItem =~ /(^|\\s)disabled(\\s|$)/",
          "group": "3_destructive@0"
        },
        {
          "command": "jenkinsWorkbench.disableJob",
          "when": "view == jenkinsWorkbench.tree && viewItem =~ /(^|\\s)(jobItem|pipelineItem)(\\s|$)/ && viewItem =~ /(^|\\s)enabled(\\s|$)/",
          "group": "3_destructive@0"
        },
        {
          "command": "jenkinsWorkbench.renameJob",
          "when": "view == jenkinsWorkbench.tree && viewItem =~ /(^|\\s)(jobItem|pipelineItem)(\\s|$)/",
          "group": "3_destructive@1"
        },
        {
          "command": "jenkinsWorkbench.copyJob",
          "when": "view == jenkinsWorkbench.tree && viewItem =~ /(^|\\s)(jobItem|pipelineItem)(\\s|$)/",
          "group": "3_destructive@2"
        },
        {
          "command": "jenkinsWorkbench.deleteJob",
          "when": "view == jenkinsWorkbench.tree && viewItem =~ /(^|\\s)(jobItem|pipelineItem)(\\s|$)/",
          "group": "3_destructive@3"
        },
        {
          "command": "jenkinsWorkbench.watchJob",
          "when": "view == jenkinsWorkbench.tree && viewItem =~ /(^|\\s)(jobItem|pipelineItem)(\\s|$)/ && !(viewItem =~ /(^|\\s)watched(\\s|$)/)",
          "group": "2_watch@0"
        },
        {
          "command": "jenkinsWorkbench.unwatchJob",
          "when": "view == jenkinsWorkbench.tree && viewItem =~ /(^|\\s)(jobItem|pipelineItem)(\\s|$)/ && viewItem =~ /(^|\\s)watched(\\s|$)/",
          "group": "2_watch@0"
        },
        {
          "command": "jenkinsWorkbench.abortBuild",
          "when": "view == jenkinsWorkbench.tree && viewItem =~ /(^|\\s)buildRunning(\\s|$)/",
          "group": "inline@0"
        },
        {
          "command": "jenkinsWorkbench.previewBuildLog",
          "when": "view == jenkinsWorkbench.tree && viewItem =~ /(^|\\s)(build|buildRunning)(\\s|$)/",
          "group": "inline@1"
        },
        {
          "command": "jenkinsWorkbench.approveInput",
          "when": "view == jenkinsWorkbench.tree && viewItem =~ /(^|\\s)awaitingInput(\\s|$)/",
          "group": "inline@1"
        },
        {
          "command": "jenkinsWorkbench.rejectInput",
          "when": "view == jenkinsWorkbench.tree && viewItem =~ /(^|\\s)awaitingInput(\\s|$)/",
          "group": "inline@2"
        },
        {
          "command": "jenkinsWorkbench.cancelQueueItem",
          "when": "view == jenkinsWorkbench.tree && viewItem == queueItem",
          "group": "inline@0"
        },
        {
          "command": "jenkinsWorkbench.previewArtifact",
          "when": "view == jenkinsWorkbench.tree && viewItem == artifactItem",
          "group": "inline@0"
        },
        {
          "command": "jenkinsWorkbench.downloadArtifact",
          "when": "view == jenkinsWorkbench.tree && viewItem == artifactItem",
          "group": "inline@1"
        },
        {
          "command": "jenkinsWorkbench.showBuildDetails",
          "when": "view == jenkinsWorkbench.tree && viewItem =~ /(^|\\s)(build|buildRunning)(\\s|$)/",
          "group": "1_actions@0"
        },
        {
          "command": "jenkinsWorkbench.openInJenkins",
          "when": "view == jenkinsWorkbench.tree && viewItem =~ /(^|\\s)(build|buildRunning)(\\s|$)/",
          "group": "1_actions@1"
        },
        {
          "command": "jenkinsWorkbench.replayBuild",
          "when": "view == jenkinsWorkbench.tree && viewItem =~ /(^|\\s)(build|buildRunning)(\\s|$)/",
          "group": "2_rebuild@0"
        },
        {
          "command": "jenkinsWorkbench.rebuildBuild",
          "when": "view == jenkinsWorkbench.tree && viewItem =~ /(^|\\s)(build|buildRunning)(\\s|$)/",
          "group": "2_rebuild@1"
        },
        {
          "command": "jenkinsWorkbench.filterBranches",
          "when": "view == jenkinsWorkbench.tree && viewItem == multibranchFolder",
          "group": "navigation@0"
        },
        {
          "command": "jenkinsWorkbench.clearBranchFilter",
          "when": "view == jenkinsWorkbench.tree && viewItem == multibranchFolder",
          "group": "navigation@1"
        }
      ],
      "editor/title": [
        {
          "command": "jenkinsWorkbench.submitJobConfig",
          "when": "resourceScheme == jenkins-config",
          "group": "navigation"
        }
      ],
      "editor/context": [
        {
          "command": "jenkinsWorkbench.jenkinsfile.validateActive",
          "when": "jenkinsWorkbench.jenkinsfileActive",
          "group": "navigation@10"
        }
      ],
      "commandPalette": [
        {
          "command": "jenkinsWorkbench.submitJobConfig",
          "when": "resourceScheme == jenkins-config"
        },
        {
          "command": "jenkinsWorkbench.enableJob",
          "when": "view == jenkinsWorkbench.tree"
        },
        {
          "command": "jenkinsWorkbench.disableJob",
          "when": "view == jenkinsWorkbench.tree"
        },
        {
          "command": "jenkinsWorkbench.renameJob",
          "when": "view == jenkinsWorkbench.tree"
        },
        {
          "command": "jenkinsWorkbench.deleteJob",
          "when": "view == jenkinsWorkbench.tree"
        },
        {
          "command": "jenkinsWorkbench.copyJob",
          "when": "view == jenkinsWorkbench.tree"
        },
        {
          "command": "jenkinsWorkbench.jenkinsfile.validateActive"
        },
        {
          "command": "jenkinsWorkbench.jenkinsfile.selectValidationEnvironment"
        },
        {
          "command": "jenkinsWorkbench.jenkinsfile.clearDiagnostics"
        },
        {
          "command": "jenkinsWorkbench.jenkinsfile.showValidationOutput"
        }
      ]
    },
